<h1>Posts#post</h1>
<input type="button" onclick="location.href='/posts/new'" value="新規作成">
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>タイトル</th>
            <th>開始日</th>
            <th>終了日</th>
            <th>終日</th>
            <th>更新日時</th>
        </tr>
    </thead>

    <tbody>
        <% @posts.each do |post| %>
        <tr>
            <td><%= post.id %></td>
            <td><%= post.title %></td>
            <td><%= post.start %></td>
            <td><%= post.end %></td>
            <td><%= post.all %></td>
            <td><%= post.updated_at %></td>
        </tr>
    </tbody>
</table>
<% end %>
<p>Find me in app/views/posts/post.html.erb</p>
